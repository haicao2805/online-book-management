@page
@model LockoutModel
@{
    ViewData["Title"] = "Locked out";
}

<header>
    <div style="padding: 80px; margin-left: 300px; height: 500px">
        <h1 class="text-black-50">@ViewData["Title"] :( </h1>
        <br/>
        <p class="text-danger font-size-5">This account has been locked out, please try again later.</p>
    </div>
</header>
